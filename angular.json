{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "fullswing": {
      "root": "apps/fullswing",
      "sourceRoot": "apps/fullswing/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/fullswing",
            "index": "apps/fullswing/src/index.html",
            "main": "apps/fullswing/src/main.ts",
            "tsConfig": "apps/fullswing/tsconfig.app.json",
            "polyfills": "apps/fullswing/src/polyfills.ts",
            "assets": ["apps/fullswing/src/assets", "apps/fullswing/src/favicon.ico"],
            "styles": ["apps/fullswing/src/styles.scss"],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "fileReplacements": [
                {
                  "replace": "apps/fullswing/src/environments/environment.ts",
                  "with": "apps/fullswing/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fullswing:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fullswing:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fullswing:build"
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/fullswing/jest.config.js",
            "tsConfig": "apps/fullswing/tsconfig.spec.json",
            "setupFile": "apps/fullswing/src/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/fullswing/tsconfig.app.json", "apps/fullswing/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "**/@fuse/**"]
          }
        }
      },
      "prefix": "jfc"
    },
