      "@app/nz-business-api": ["libs/nz-business-api/src/index.ts"],
      "@app/copper-crm": ["libs/copper-crm/src/index.ts"],
      "@app/chat-components": ["libs/chat-components/src/index.ts"],
      "@reusable-parts/fuse": ["libs/fuse/index.ts"],
      "@app/storybook": ["libs/storybook/src/index.ts"],
      "@app/stateless/user-toolbar": ["libs/stateless/user-toolbar/src/index.ts"],
      "@app/stateless/welcome": ["libs/stateless/welcome/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
